pipeline {
     agent alpha 
          stage {
                   stage ('compile stage'){
                        step {
                              withMaven (maven: 'maven_3_5_2'){
                           sh 'mvn clean compile' 
                      }
                   }
                                
                }
                     stage ('package stage'){
                                step {
                             withMaven (maven: 'maven_3_5_2'){
                                        sh 'mvn package'
                      }
                   
                  }
               }


}
 
}
